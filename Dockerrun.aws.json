{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "svelte",
      "image": "568650432135.dkr.ecr.us-east-1.amazonaws.com/nui_frontend:nui_frontend",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 5000,
          "containerPort": 35729
        }
      ],
      "links": [
        "redis"
      ]
    },
    {
      "name": "nodejs",
      "image": "568650432135.dkr.ecr.us-east-1.amazonaws.com/nui_backend:nui_backend",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 3000,
          "containerPort": 3000
        }
      ]
    }
  ]
}
