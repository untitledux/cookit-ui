{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "dvwa",
      "image": "vulnerables/web-dvwa",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ]
    },
    {
      "name": "yeasy",
      "image": "yeasy/simple-web",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 8080,
          "containerPort": 80
        }
      ]
    }
  ]
}
